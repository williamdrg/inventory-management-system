{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["./src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "path": "node_modules/clean-jsdoc-theme",
    "theme": "light",
    "navType": "vertical",
    "linenums": true,
    "collapseSymbols": false,
    "inverseNav": true,
    "outputSourcePath": true,
    "outputSourceFiles": true
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs",
    "recurse": true,
    "verbose": true,
    "readme": "./Instrucciones.md",
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "static_dir": ["./static"],
    "favicon": "/static/logoPrueba.png",
    "homepageTitle": "API Inventario",
    "title": "Documentaci√≥n"
  }
  },
  "markdown" : {
    "hardwrap" : false ,
    "idInHeadings" : true 
  }
}
